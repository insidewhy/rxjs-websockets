{
  "name": "rxjs-websockets",
  "version": "7.0.0",
  "description": "rxjs 6 websockets library",
  "main": "lib/index.js",
  "typings": "lib/index",
  "repository": {
    "type": "git",
    "url": "git://github.com/ohjames/rxjs-websockets.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "redux",
    "redux-observable",
    "rxjs",
    "vuex",
    "websocket",
    "websockets"
  ],
  "scripts": {
    "build": "tsc -p src",
    "build:watch": "tsc -w --preserveWatchOutput -p src",
    "test": "npm run build && npm run mocha",
    "watch": "concurrently 'npm run build:watch' 'npm run mocha:watch'",
    "prepare": "npm run build",
    "mocha": "mocha lib/*.spec.js",
    "mocha:watch": "npm run mocha -- -w"
  },
  "author": "James Pike <github@chilon.net>",
  "license": "ISC",
  "peerDependencies": {
    "rxjs": "^6.1.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.6",
    "@types/sinon": "7.0.10",
    "@types/sinon-chai": "3.2.2",
    "chai": "4.2.0",
    "concurrently": "4.1.0",
    "mocha": "6.0.2",
    "rxjs": "6.4.0",
    "sinon": "7.3.1",
    "sinon-chai": "3.3.0",
    "typescript": "3.3.4000"
  }
}
